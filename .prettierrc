{
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "auto",
  "htmlWhitespaceSensitivity": "css",
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "printWidth": 100,
  "proseWrap": "preserve",
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": true,
  "semi": false,
  "singleQuote": true,
  "disableLanguages": [
    "vue",
    "json"
  ]
}