{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"lib": ["ESNext", "DOM"],
		"moduleResolution": "Node",
		"sourceMap": true,
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"types": ["vite/client"],
		"noEmit": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true
	},
	"include": ["./src"]
}
